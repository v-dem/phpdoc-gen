{
    "name": "v-dem/phpdoc-gen",
    "description": "Class reference documentation generator using PHPDoc",
    "homepage": "https://github.com/v-dem/phpdoc-gen/",
    "keywords": ["php", "phpdoc", "documentation", "markdown"],
    "license": "LGPL-3.0",
    "minimum-stability": "dev",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Vitaly Demyanenko",
            "email": "vitaly_demyanenko@yahoo.com"
        }
    ],
    "require": {
        "php": ">=5.3.0|>=7.0.0"
    },
    "require-dev": {
        "php": ">=5.6.0|>=7.0.0",
        "phpunit/phpunit": "~5.7"
    },
    "autoload":{
        "psr-4":{
            "vdem\\phpdocgen\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "vdem\\phpdocgen\\tests\\": "tests/src/"
        }
    }
}

